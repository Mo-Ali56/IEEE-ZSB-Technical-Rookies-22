lst = []
print("Enter the arr elements number ")
x = int(input())
y=0
k=0
while (y<x):
    y=y+1
    if k == 0:
        print("then enter the first num") 
        k=k+1
    else:
        print("the next num")    
    num=int(input())
    lst.append(num)
z=sum(lst) 
print("the sum off arr is " , z)   
